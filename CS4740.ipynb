    "from google.colab import drive\n",
    "import pandas as pd\n",
    "import os\n",
    "from sklearn.model_selection import train_test_split\n",
    "import shutil\n",
    "\n",
    "# Mount Google Drive\n",
    "drive.mount('/content/drive')\n",
    "\n",
    "# Assuming 'thedata' folder is at the root of your Drive. Update the path if it's different.\n",
    "base_dir = '/content/drive/MyDrive/thedata'\n",
    "metadata_path = os.path.join(base_dir, 'metadata.csv')\n",
    "\n",
    "# Load metadata into a DataFrame\n",
    "metadata = pd.read_csv(metadata_path)\n",
    "\n",
    "# Display the first few entries to ensure it's loaded correctly\n",
    "print(metadata.head())\n"
